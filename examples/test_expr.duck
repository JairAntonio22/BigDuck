var call_count int;

proc fib(n int) -> int {
    call_count <- call_count + 1;

    if n <= 0 {
        return 0;

    } else if n = 1 {
        return 1;

    } else {
        return fib(n - 1) + fib(n - 2);
    }
}

proc main()
    var ans, i int;
{
    loop i <- 0; i <= 35; i <- i + 1 { 
        call_count <- 0;
        ans <- fib(i);
        print("The", i, "fibonacci is", ans, "took", call_count, "iterations");
    }
}
