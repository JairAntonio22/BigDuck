
proc fib(n int) -> int {
    if n <= 0 {
        return 0;
    } else if n = 1 {
        return 1;
    } else {
        return fib(n - 1) + fib(n - 2);
    }
}
#| main function |#
proc main() {
    print(fib(10));
}

#|
proc abs(x float) -> float {
    if x < 0 {
        return x * -1;
    } else {
        return x;
    }
}

proc main() 
    var x, y float;
{
    x <- 2;
    y <- 1;

    loop abs(y * y - x) > 0.00001 {
        y <- (x / y + y) / 2;
    }

    print(y);
}
