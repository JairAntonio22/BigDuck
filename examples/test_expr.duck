
proc abs(x float) -> float {
    if x < 0 {
        return x * -1;
    } else {
        return x;
    }
}

proc sqrt(x float) -> float
    var y float;
{
    y <- 1;

    loop abs(y * y - x) > 0.00001 {
        y <- (x / y + y) / 2;
    }

    return y;
}

proc main() {
    print(sqrt(2));
    print(sqrt(4));
    print(sqrt(10));
}
